{
  "name": "kalkbrenner",
  "version": "1.0.0",
  "description": "Website of KFZ Kalkbrenner",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:del": "rimraf dist",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --watch",
    "dev:webpack": "webpack-dev-server --mode development --watch",
    "build": "run-s prod:*",
    "now-build": "run-s prod:*",
    "prod:del": "rimraf build",
    "prod:webpack": "webpack",
    "prod:serve": "ELEVENTY_ENV=production eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simple-bytes/kalkbrenner.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simple-bytes/kalkbrenner/issues"
  },
  "homepage": "https://github.com/simple-bytes/kalkbrenner#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "babel-loader": "^8.0.6",
    "bulma": "^0.7.5",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "eleventy-plugin-nesting-toc": "^1.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "luxon": "^1.16.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
