{
  "name": "pension-elli",
  "version": "1.0.0",
  "description": "Website of KFZ pension-elli",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:del": "rimraf web",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --watch",
    "dev:webpack": "webpack-dev-server --mode development --watch",
    "build": "run-s prod:*",
    "prod:del": "rimraf web",
    "prod:webpack": "webpack --mode production",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy",
    "deploy": "rsync -avz web/* web-server:/var/www/kalkbrenner/web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simple-bytes/pension-elli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simple-bytes/pension-elli/issues"
  },
  "homepage": "https://github.com/simple-bytes/pension-elli#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "babel-loader": "^8.0.6",
    "bulma": "^0.7.5",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "eleventy-plugin-nesting-toc": "^1.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "luxon": "^1.16.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "flickity": "^2.2.1",
    "google-maps": "^3.3.0",
    "headroom.js": "^0.9.4",
    "scrollreveal": "^4.0.5"
  }
}
